<%- contentFor('Content') %>
    <h1>Contact Me</h1>
    <% if (cont.length>0) { %>
        <table class="table">
            <tr>
                <th>Name</th>
                <th>Email</th>
            </tr>
            <% cont.forEach(element=> { %>
                <tr>
                    <td><%= element.name %></td>
                    <td><%= element.email %></td>
                </tr>
            <% }) %>
    <% } else { %>
            <p>Data tidak ditemukan</p>
    <% } %>
        </table>